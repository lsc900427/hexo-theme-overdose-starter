      <category term="desturcturing" scheme="https://GITHUB_ID.github.io/tags/desturcturing/"/>
    
      <category term="해체할당" scheme="https://GITHUB_ID.github.io/tags/%ED%95%B4%EC%B2%B4%ED%95%A0%EB%8B%B9/"/>
    
